    "import numpy as np\n",
    "\n",
    "# Generate some sample data\n",
    "x = np.array([1, 2, 3, 4, 5])\n",
    "y = np.array([2, 3, 4, 5, 6])\n",
    "\n",
    "# Solve for m (slope) and c (intercept)\n",
    "m, c = np.polyfit(x, y, 1)\n",
    "\n",
    "print(\"Slope (m):\", m)\n",
    "print(\"Y-intercept (c):\", c)\n"
   ]
  }
